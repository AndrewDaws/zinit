tst_verbosity

zinit ice depth"1" atpull"command chmod -x zsh-select"
zinit load psprint/zsh-select

print -rl -- "${(o)ZINIT_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZINIT_SICE[@]}" >! answer/sice

perl -pi -e 's/test_plugins\/zsh-select/test_plugins\/zsh-select-more/g' answer/plugins/psprint---zsh-select/.git/config

( zinit cd psprint/zsh-select
  set -e
  print 'touch Test_has_failed' >! ._zinit/atpull
  print 'LICENSE -> LICENSE.bkp' >! ._zinit/cp ) || return 1

zinit update psprint/zsh-select

