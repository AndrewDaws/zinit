tst_verbosity

zinit ice depth"1" atpull'!command echo local\ change >! version4' cp'version4 -> version4.cpy'
zinit load psprint/zsh-select

print -rl -- "${(o)ZPLG_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZPLG_SICE[@]}" >! answer/sice

perl -pi -e 's/test_plugins\/zsh-select/test_plugins\/zsh-select-more/g' answer/plugins/psprint---zsh-select/.git/config

( zinit cd psprint/zsh-select
  set -e
  print 'touch Test_has_failed' >! ._zinit/atpull ) || return 1

zinit update psprint/zsh-select

( zinit cd psprint/zsh-select
  git log --oneline >! git_log )

