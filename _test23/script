tst_verbosity

zinit ice as"command" mv"Makefile2 -> Makefile" cp"Makefile -> Makefile.copy" \
            atclone"cp -v Makefile.copy Makefile.copy2" make \
            atinit"cp -v Makefile.copy3 Makefile.copy4" \
            atload"cp -v Makefile.copy4 Makefile.copy5" pick"Makefile.copy4" 
zinit snippet OMZ::plugins/make/Makefile2

print -rl -- "${(o)ZINIT_ICE[@]}" >! answer/ice
print -rl -- "${(okv)ZINIT_SICE[@]}" >! answer/sice

